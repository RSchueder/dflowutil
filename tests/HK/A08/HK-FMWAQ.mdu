TStop                             = 63072000.           # Stop  time w.r.t. RefDate (in TUnit)

[restart]
RestartFile                       =                     # Restart netcdf-file, either *_rst.nc or *_map.nc
RestartDateTime                   =                     # Restart date and time (yyyymmddhhmmss) when restarting from *_map.nc

